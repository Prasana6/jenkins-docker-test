pipeline {
    agent any

    stages {
        stage('Docker-Build') {
            steps {
                sh 'sudo docker build -t prasannabaskar/testing:coda .'
                sh 'sudo docker push prasannabaskar/testing:coda'
            }
        }
        stage('Cleanup') {
            steps {
                sh 'docker system prune --all -f'
            }
        }
    }
}
